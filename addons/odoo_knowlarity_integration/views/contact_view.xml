<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="whatsapp_res_view_form" model="ir.ui.view">
            <field name="name">knowlarity.call.wizard.form.inherit.res</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='image_1920']" position="before">
                    <div class="oe_button_box" name="button_box">
                        <button name="contacts_form_view" string="Knowlarity Call" type="object" class="oe_highlight"
                                icon="fa-phone">
                        </button>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>